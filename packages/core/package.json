{
  "name": "@lumina/core",
  "type": "module",
  "version": "0.0.0",
  "description": "Lumina is a free and open-source discord bot framework focused on type-safety and ease of use.",
  "author": "quantix-dev",
  "license": "LGPL-3.0-or-later",
  "homepage": "https://lumina.quantix.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quantix-dev/lumina.git",
    "directory": "packages/core"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": {
        "types": "./dist/config.d.mts",
        "default": "./dist/config.mjs"
      },
      "require": {
        "types": "./dist/config.d.cts",
        "default": "./dist/config.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "lumina": "./bin/lumina.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "prepack": "unbuild"
  },
  "peerDependencies": {
    "discord.js": "^14.15.3"
  },
  "dependencies": {
    "discord.js": "^14.15.3",
    "@lumina/cli": "latest"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^6.0.4",
    "zlib-sync": "^0.1.9"
  },
  "devDependencies": {
    "unbuild": "^2.0.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
